# Raspberry Pi Deployment Configuration
# Copy this file to .env and fill in your values

# Pi connection details
PI_HOSTNAME=raspberrypi.local
PI_USERNAME=pi
PI_PASSWORD=your-password-here

# Deployment paths
PI_DEPLOY_PATH=/home/pi/home-library

# Docker image names
BACKEND_IMAGE=home-library-backend:latest
FRONTEND_IMAGE=home-library-frontend:latest
