{
    "compilerOptions": {
        "types": ["node", "@playwright/test"],
        "target": "ESNext",
        "module": "ESNext",
        "lib": ["ESNext", "DOM"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "noEmit": true,
        "baseUrl": "./"
    },
    "include": ["e2e/**/*"]
}